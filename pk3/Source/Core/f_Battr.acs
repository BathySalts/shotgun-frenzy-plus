// battery warnings
script 111 (int l_Type)
{
    // battery
    if (l_Type == 0)
    {
        f_CenterTipBold("Battery Under Attack!");
    }
    
    // core
    if (l_Type == 1)
    {
        f_CenterTipBold("Core Under Attack!");
    }
}